#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/segeon/Documents/GitHub/Bento/FrontEnd/node_modules/.pnpm/svelte-check@4.1.4_svelte@5.20.1_typescript@5.7.3/node_modules/svelte-check/bin/node_modules:/Users/segeon/Documents/GitHub/Bento/FrontEnd/node_modules/.pnpm/svelte-check@4.1.4_svelte@5.20.1_typescript@5.7.3/node_modules/svelte-check/node_modules:/Users/segeon/Documents/GitHub/Bento/FrontEnd/node_modules/.pnpm/svelte-check@4.1.4_svelte@5.20.1_typescript@5.7.3/node_modules:/Users/segeon/Documents/GitHub/Bento/FrontEnd/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/segeon/Documents/GitHub/Bento/FrontEnd/node_modules/.pnpm/svelte-check@4.1.4_svelte@5.20.1_typescript@5.7.3/node_modules/svelte-check/bin/node_modules:/Users/segeon/Documents/GitHub/Bento/FrontEnd/node_modules/.pnpm/svelte-check@4.1.4_svelte@5.20.1_typescript@5.7.3/node_modules/svelte-check/node_modules:/Users/segeon/Documents/GitHub/Bento/FrontEnd/node_modules/.pnpm/svelte-check@4.1.4_svelte@5.20.1_typescript@5.7.3/node_modules:/Users/segeon/Documents/GitHub/Bento/FrontEnd/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../svelte-check/bin/svelte-check" "$@"
else
  exec node  "$basedir/../svelte-check/bin/svelte-check" "$@"
fi
